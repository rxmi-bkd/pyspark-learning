   "source": [
    "import pyspark.sql.functions as F\n",
    "\n",
    "from pyspark.sql import SparkSession\n",
    "\n",
    "data = [[[1, 2, 3]]]\n",
    "\n",
    "spark = SparkSession.builder.getOrCreate()\n",
    "\n",
    "df = spark.createDataFrame(data, ['nums'])\n",
    "df = df.select('nums', F.explode('nums').alias('num'))\n",
    "df.show()"
   ]
