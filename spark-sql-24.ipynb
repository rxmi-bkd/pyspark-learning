    "from pyspark.sql import SparkSession\n",
    "\n",
    "spark = SparkSession.builder.getOrCreate()\n",
    "\n",
    "data = [\n",
    "    [0, 0],\n",
    "    [1, 1],\n",
    "    [2, 0],\n",
    "    [3, 1],\n",
    "    [4, 0]\n",
    "]\n",
    "\n",
    "df = spark.createDataFrame(data, ['value', 'group'])\n",
    "df = df.groupBy('group').max('value')\n",
    "df.show()"
   ]
