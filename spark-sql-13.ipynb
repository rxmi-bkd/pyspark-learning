   ],
   "source": [
    "from pyspark.sql import SparkSession\n",
    "\n",
    "data = [\n",
    "    [['a', 'b', 'c']],\n",
    "    [['X', 'Y', 'Z']]\n",
    "]\n",
    "\n",
    "spark = SparkSession.builder.getOrCreate()\n",
    "\n",
    "df = spark.createDataFrame(data, ['values'])\n",
    "\n",
    "rdd = df.rdd.flatMap(lambda row: [row.values])\n",
    "\n",
    "df = rdd.toDF(['col1', 'col2', 'col3'])\n",
    "df.show()"
   ]
